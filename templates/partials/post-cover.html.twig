<div class="cover b-lazy" data-src="{{ page.media['cover.jpg'].url }}" data-src-small="{{ page.media['cover.jpg'].cropResize(750,420).url }}">
</div>
<div class="meta">
  <div class="wrap sqncbrk-outer">
    <time class="date" datetime="{{ page.date|date('Y-m-d') }}">{{ 'SQNC_MONTHS'|ta(page.date|date('n') - 1) }} {{ page.date|date('d') }}, {{ page.date|date('Y') }}</time>
    <h2 class="title">{{ page.title }}</h2>
  </div>
</div>
